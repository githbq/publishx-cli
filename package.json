{
    "name": "publishx-cli",
    "version": "1.6.32",
    "description": "publishx-cli 方便提交git库的命令 多功能合一命令，辅助多项目开发",
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "main": "./build/command",
    "bin": {
        "publishx": "./bin.js",
        "px": "./bin.js"
    },
    "scripts": {
        "start": "node run",
        "clear": "rimraf build",
        "tsc": "npm run clear && tsc",
        "tsc:w": "npm run clear && tsc -w",
        "p": "npm run tsc && npm publish",
        "lint": "tslint ./src/**/*.ts --fix",
        "postpublish": "npm i -g publishx-cli --registry=https://registry.npmjs.org"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "bluebird": "^3.5.4",
        "cli-color": "^1.4.0",
        "debug": "^2.6.6",
        "delay": "^2.0.0",
        "download": "^6.2.5",
        "fs-extra": "^8.1.0",
        "fs-extra-promise": "^1.0.1",
        "globby": "^6.1.0",
        "globby-compress": "^1.0.9",
        "inquirer": "^7.3.3",
        "isos": "^0.0.3",
        "json-stringify-pretty-compact": "^1.0.4",
        "listr": "^0.14.3",
        "lodash": "^4.17.4",
        "moment-helper": "^0.0.1",
        "ora": "^3.4.0",
        "pretty-ms": "^2.1.0",
        "require-dir": "^0.3.2",
        "rimraf": "^2.6.2",
        "rxjs": "^6.5.2",
        "semver": "^5.3.0",
        "spawn-helper": "^1.0.0",
        "tslib": "^1.14.1",
        "yargs": "^7.1.0"
    },
    "devDependencies": {
        "@types/bluebird": "^3.5.3",
        "@types/cli-color": "^0.3.29",
        "@types/fs-extra": "^8.0.0",
        "@types/lodash": "^4.14.123",
        "@types/node": "^6.0.48",
        "@types/semver": "^5.3.31",
        "ts-node": "^3.3.0",
        "tslint": "^5.2.0",
        "typescript": "^4.1.3"
    },
    "keywords": [
        "typescript",
        "easy git",
        "npmjs",
        "auto version",
        "auto tool",
        "git config",
        "git helper",
        "cli"
    ],
    "engines": {"node": ">=7.6"},
    "author": "hubiqing",
    "repository": "https://github.com/githbq/publishx-cli.git",
    "license": "MIT",
    "enginxConfig": {
        "serverRootPath": "/export/local/www/",
        "appName": "/Users/hubiqing/Documents/github-workspace/publishx-cli",
        "backendHost": "127.0.0.1"
    }
}