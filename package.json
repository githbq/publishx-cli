{
    "name": "publishx-cli",
    "version": "1.2.2",
    "description": "publishx-cli 方便提交git库的命令 多功能合一命令，辅助多项目开发",
    "main": "./build/command",
    "bin": {
        "publishx": "./bin.js",
        "px": "./bin.js"
    },
    "scripts": {
        "start": "node run",
        "clear": "rimraf build",
        "tsc": "npm run clear && tsc",
        "tsc:w": "npm run clear && tsc -w",
        "p": "npm run tsc && npm publish",
        "lint": "tslint \"src/**/*.ts\""
    },
    "dependencies": {
        "cli-color": "^1.2.0",
        "debug": "^2.6.6",
        "delay": "^2.0.0",
        "fs-extra-promise": "^1.0.1",
        "globby": "^6.1.0",
        "json-stringify-pretty-compact": "^1.0.4",
        "listr": "^0.12.0",
        "lodash": "^4.17.4",
        "ora": "^1.2.0",
        "pretty-ms": "^2.1.0",
        "prompt-promise2": "^0.0.1",
        "require-dir": "^0.3.1",
        "rxjs": "^5.4.0",
        "semver": "^5.3.0",
        "spawn-helper": "^0.0.9",
        "tslib": "^1.6.1",
        "yargs": "^7.1.0"
    },
    "devDependencies": {
        "@types/bluebird": "^3.5.3",
        "@types/cli-color": "^0.3.29",
        "@types/fs-extra-promise": "^1.0.1",
        "@types/lodash": "^4.14.64",
        "@types/node": "^6.0.48",
        "@types/semver": "^5.3.31",
        "ts-node": "^3.0.2",
        "tslint": "^5.2.0",
        "typescript": "^2.3.2"
    },
    "keywords": [
        "typescript",
        "easy git",
        "npmjs",
        "auto version"
    ],
    "engines": {"node": ">=7.6"},
    "author": "hubiqing",
    "repository": "git@github.com:githbq/publishx-cli.git",
    "license": "MIT"
}